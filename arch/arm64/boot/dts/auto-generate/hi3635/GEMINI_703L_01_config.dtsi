		/* I2C0 ~ I2C4
		Notation for touchscreen and other devices, that is not defined in I2C DTS node:
		if any I2C adapter is disabled, corresponding I2C ID should decreased one by
		one, for example:
			if only I2C0 is disabled, slave devices on I2C1 should get adapter by ID 0
			if only I2C1 is disabled, slave devices on I2C2 should get adapter by ID 1
			and so on........
		*/
		i2c0: i2c@FDF09000 {
			status = "disabled";
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&gpio047_pmx_func &gpio047_cfg_func &gpio048_cfg_func>;
			pinctrl-1 = <&gpio047_pmx_idle &gpio047_cfg_idle &gpio048_cfg_idle>;
			cs-gpios = <&gpio5 7 0>, <&gpio6 0 0>;
		};

		i2c1: i2c@FDF0A000 {
			status = "ok";
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&gpio049_pmx_func &gpio049_cfg_func &gpio050_cfg_func>;
			pinctrl-1 = <&gpio049_pmx_idle &gpio049_cfg_idle &gpio050_cfg_idle>;
			cs-gpios = <&gpio6 1 0>, <&gpio6 2 0>;

			huawei,lm3642@63{
				huawei,flash_current = <0x9>;
				huawei,torch_current = <0x3>;
				status = "ok";
			};

			tps65132_phy: tps65132@3e {
			    compatible = "hisilicon,tps65132_phy";
			    reg = <0x3e>;
			    gpios = <&gpio5 1 0>, <&gpio5 2 0>;
			    status = "ok";
			};
			fairchild_fsa9685: fsa9685@25 {
				 status = "disabled";
			};
			bq2419x_charger:charger@6B {
				status = "disable";
			};
			bq2429x_charger:bq2429x_charger@6B {
				status = "ok";
			};
			max98925_smartpa:smartpa@31{
				status = "ok";
			};
			pn547_nfc:nfc@28 {
				status = "disabled";
				nfc_nxp_name = "libnfc-nxp.conf";
			};
		};

		i2c2: i2c@FDF0B000 {
			status = "ok";
			pinctrl-names = "default", "idle";
			pinctrl-0 = <&gpio051_pmx_func &gpio051_cfg_func &gpio052_cfg_func>;
			pinctrl-1 = <&gpio051_pmx_idle &gpio051_cfg_idle &gpio052_cfg_idle>;
			cs-gpios = <&gpio6 3 0>, <&gpio6 4 0>;

                        cypress_touchscreen@24 {
                               compatible = "cy,cyttsp5_i2c_adapter";
                               reg = <0x24>;
                               cy,adapter_id = "cyttsp5_i2c_adapter";

			       vci_power_type   = <0>; /*0 - gpio control  1 - LDO  2 - boost & gpio*/
                               cypress_vci-supply;
			       vci_gpio_enable  = <&gpio9 1 0>;
			       vci_boost_enable;

			       vddio_power_type   = <1>; /*0 - gpio control  1 - LDO*/
                               cypress_vddio-supply = <&lsw50>;
			       vddio_gpio_enable;
                               
			       cy,i2c_read_length = <100>;
			       pinctrl-names = "default", "idle";
		               pinctrl-0 = <&gpio200_pmx_func  &gpio200_cfg_func>;
                               pinctrl-1 = <&gpio200_pmx_idle  &gpio200_cfg_idle>; 

				hw,mmi_test_limit =
					"0","255","6","0","255","6","0","255","6","0","255","6",                     /*mmi_idac_limit*/
					"79","120","79","120","79","120","75","120",                                 /*mmi_localPWC_limit*/
					"25","4","8", "21","16","10","200","25","5","9", "21","16","10","200",       /*mmi_opens_limit*/
					"25","4","10","21","16","10","200","25","6","10","21","16","10","200",       /*mmi_opens_limit*/
					"-2000","2000","0","-2000","2000","0","-2000","2000","0","-2000","2000","0", /*mmi_mut_rawdata_limit*/
					"-7500","7500","0","-7500","7500","0","-7500","7500","0","-7500","7500","0", /*mmi_self_rawdata_limit*/
					"10","35","255","0","27","10","35","255","0","27",                           /*mmi_selflocalPWC_limit*/
					"10","35","255","0","27","10","35","255","0","30";                           /*mmi_selflocalPWC_limit*/

                               cy,core {
                                       cy,id = "main_ttsp_core";
                                       cy,name = "cyttsp5_core";
                                       cy,irq_gpio = <&gpio25 0 0>;
                                       cy,rst_gpio = <&gpio12 2 0>;
                                       cy,hid_desc_register = <1>;
				       cy,product_name = "Gemini";
				       cy,support_chip_name = "TMA568";
                                       cy,flags = <0>;/*CY_CORE_FLAG_WAKE_ON_GESTURE | CY_CORE_FLAG_RESTORE_PARAMETERS*/
                                       cy,easy_wakeup_gesture = <0>;
                                       cy,upgrade_ttconfig = <0>;
                                       cy,has_touchplus = <1>;
                                       cy,has_holster = <1>;
                                       cy,no_cali_flag = <1>;

			               cy,btn_keys =
                                       <189   /* TS_TOUCHPLUS_KEY0 = KEY_F21, //189 */
				        190   /* TS_TOUCHPLUS_KEY1 = KEY_F22, //190 */
				        191   /* TS_TOUCHPLUS_KEY2 = KEY_F23, //191 */
				        192   /* TS_TOUCHPLUS_KEY3 = KEY_F19, //192 */
				        193>; /* TS_TOUCHPLUS_KEY4 = KEY_F20, //193 */
                                       cy,btn_keys-tag = <0>;
                               cy,mt   {
                                       cy,name = "cyttsp5_mt";
                                       cy,inp_dev_name = "cyttsp5_mt";
                                       cy,flags = <0x00>;/* CY_MT_FLAG_FLIP | CY_MT_FLAG_INV_X | CY_MT_FLAG_INV_Y */
                                       cy,abs =
                                       <0x35   0 1200 0 0           /* ABS_MT_POSITION_X, CY_ABS_MIN_X, CY_ABS_MAX_X, 0, 0 */
                                        0x36   0 1920 0 0           /* ABS_MT_POSITION_Y, CY_ABS_MIN_Y, CY_ABS_MAX_Y, 0, 0 */
                                        0x3a   0 255  0 0           /* ABS_MT_PRESSURE,   CY_ABS_MIN_P, CY_ABS_MAX_P, 0, 0 */
                                        0xffff 0 255  0 0           /* CY_IGNORE_VALUE,   CY_ABS_MIN_W, CY_ABS_MAX_W, 0, 0 */
                                        0x39   0 15   0 0           /* ABS_MT_TRACKING_ID,CY_ABS_MIN_T, CY_ABS_MAX_T, 0, 0 */
                                        0x30   0 255  0 0           /* ABS_MT_TOUCH_MAJOR,0, 255, 0, 0 */
                                        0x31   0 255  0 0           /* ABS_MT_TOUCH_MINOR,0, 255, 0, 0 */
                                        0x34   0xfffffed7 127 0 0>; /* ABS_MT_ORIENTATION,-128, 127, 0, 0 */
                                       };
			       cy,btn {
			              cy,name = "cyttsp5_btn";
				      cy,inp_dev_name = "cyttsp5_btn";
			              };
                               };
                       };   
		};

		i2c3: i2c@FDF0C000 {
